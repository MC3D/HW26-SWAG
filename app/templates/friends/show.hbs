<form class=friends-profile>
  <ul>
    <li><label>Birthday:</label>{{birthday}}</li>
    <li><label>Anniversary:</label>{{anniversary}}</li>
  </ul>

  <ul>
  {{#each this.interests}}
      <li>{{interestText}}</li>
    {{/each}}
  </ul>

  <ul>
    {{#each this.aversions}}
      <li>{{aversionText}}</li>
    {{/each}}
  </ul>

  <ul>
    <li><label>Hat Size:</label>{{hatSize}}</li>
    <li><label>Shirt Size:</label>{{shirtSize}}</li>
    <li><label>Pant Size:</label>{{pantSize}}</li>
    <li><label>Shoe Size:</label>{{shoeSize}}</li>
  </ul>
</form>



<form class='swagbag'>
  <h2>{{username}}'s Swag</h2>

  <div class='item'>
    {{#each this.swagbag}}
      <img class='imgswag' {{bind-attr src=swagURL}} />
      <ul>
        <li class='data'>{{description}}</li>
        <li class='data'>{{retailer}}</li>
        <li class='data'>{{location}}</li>
        <li class='data'>{{price}}</li>
      </ul>
    {{/each}}
  </div>

</form>
